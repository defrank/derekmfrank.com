# NAME
#   urls.py - accounts
#
# DESCRIPTION
#   A url patterns definition for user profiles.
#

from django.conf.urls import patterns, url

urlpatterns = patterns('accounts.views',
    # About me
    url(r'^$', 'aboutme'),
    # About a user
    url(r'^(?P<username>\w+)/$', 'user'),
)
@
